{
  "cli": {
    "version": ">= 8.0.0",
    "appVersionSource": "local",
    "requireCommit": true
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "ios": {
        "simulator": true,
        "enterpriseProvisioning": "adhoc"
      },
      "distribution": "internal"
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
